; Program to only print inputs that are allowed to be in memory (0-999)

entry	INP		; get user input

	BRP	check	; check not negative
	BRA	entry

; therefore the value is >= 0
check	SUB	max	; check if will exceed memory max
	SUB	one
	BRP	entry
	ADD	one	; revert subtraction for range check
	ADD	max

; therefore the value is within 0-999!
	OUT
	BRA	entry

one	DAT	1
max	DAT	999
